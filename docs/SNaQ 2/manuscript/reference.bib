@article{vanderpool2020,
  title = {Primate Phylogenomics Uncovers Multiple Rapid Radiations and Ancient Interspecific Introgression},
  author = {Vanderpool, Dan and Minh, Bui Quang and Lanfear, Robert and Hughes, Daniel and Murali, Shwetha and Harris, R. Alan and Raveendran, Muthuswamy and Muzny, Donna M. and Hibbins, Mark S. and Williamson, Robert J. and Gibbs, Richard A. and Worley, Kim C. and Rogers, Jeffrey and Hahn, Matthew W.},
