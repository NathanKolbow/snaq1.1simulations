@article{miller2020,
  title = {The Influence of a Priori Grouping on Inference of Genetic Clusters: Simulation Study and Literature Review of the {{DAPC}} Method},
  shorttitle = {The Influence of a Priori Grouping on Inference of Genetic Clusters},
  author = {Miller, Joshua M. and Cullingham, Catherine I. and Peery, Rhiannon M.},
  year = {2020},
